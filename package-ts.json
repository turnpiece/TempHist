{
  "name": "temphist",
  "version": "1.0.3",
  "description": "Daily temperature history chart. Shows the temperature on the same day each year over the past 50 years for the current location.",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest --watch",
    "start": "node server.js",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "version:show": "echo $npm_package_version"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turnpiece/TempHist.git"
  },
  "keywords": [],
  "author": "Paul Jenkins",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/turnpiece/TempHist/issues"
  },
  "homepage": "https://github.com/turnpiece/TempHist#readme",
  "dependencies": {
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "firebase": "^11.9.1",
    "http-proxy-middleware": "^3.0.5"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/chart.js": "^2.9.41",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "clean-css-cli": "^5.6.3",
    "jsdom": "^27.0.0",
    "sass": "^1.89.2",
    "typescript": "^5.0.0",
    "vite": "^7.0.0",
    "vitest": "^3.2.4"
  }
}
